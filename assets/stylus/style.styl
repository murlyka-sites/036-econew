@import 'inc/ui'
@import 'inc/alert'
@import 'inc/nav-sticky'
@import 'inc/nav-top'
@import 'inc/header'
@import 'inc/main'
@import 'inc/edge'
@import 'inc/projects'
@import 'inc/works'
@import 'inc/clients'

.SMain-Wrapper {
	&-Container {
		margin auto
	}
}

.SMainProduct {
	background #f7f7f7 url(../images/mainproduct/bg.png) left bottom / 672px auto no-repeat
	text-align center
	padding 30px 0 0
	
	&-Container {
		margin auto 
	}
	
	@media (min-width $breakpoint-lg) {
		text-align left
	}

	&-Pr {
		width auto
		height auto
		
		@media (min-width $breakpoint-md) {
			width 430px 
		}
	}
	
	&-Title {
		margin 0 0 15px
		font-size 24px 
		font-weight 300
		line-height 1
		 
		@media (min-width $breakpoint-md) {
			font-size 42px 
		}
		
		@media (min-width $breakpoint-lg) {
			font-size 48px 
			margin 0 0 25px
		}
		
		@media (min-width $breakpoint-xl) {
			font-size 52px 
		}
		
		@media (min-width $breakpoint-xxl) {
			font-size 60px 
		}
	}
	
	&-Buttons {
		
		@media (min-width 1360px) {
			max-width 530px 
		}
	}
	
	&-Button {
		display block
		width 100%
		margin-bottom 10px 
		text-transform uppercase
		padding-left 15px 
		padding-right 15px 
		
		@media (min-width $breakpoint-xl) {
			font-size 12px 
		}
	}
	
	&-Cat {
		padding 15px 
		margin-bottom 15px
		background-color: #e5e5e5;
		font-size 15px 
		
		@media (min-width $breakpoint-md) {
			background none
			padding 0
			margin-bottom 35px  
			
		}
		
		@media (min-width $breakpoint-lg) {
			font-size 18px 
		}
	}
	
	&-Inner {
	}
	
	&-Nav {
		margin-top 15px
	}
}

.ListCat {
	list-style-type none
	margin 0
	padding 0
	text-align left
	font-weight 600 
	
	&-Link {
		display inline-flex
		flex-wrap wrap
		position relative
		padding-left 15px
		hyphens auto
		
		&:hover {
			text-decoration none
		}
		
		&::before {
			content ''
			display inline-block
			width 7px 
			height 7px
			background currentColor 
			transform translate(0, -50%) rotate(45deg)
			position absolute
			left 0px
			top 0.75em
			
		}
		

	}
	

}

.CMainProductNav {
	stroke #3f9ae3
	padding 15px 20px
	color #333
	
	@media (min-width $breakpoint-md) {
		text-align left
	}
	
	&:nth-child(1) {
		background-color: #e5e5e5;
	}
	
	&:nth-child(2) {
	
	}
	
	&:nth-child(3) {
		background-color: #d8d8d8;
	}
	
	&:nth-child(4) {
		background #cccccc 
	}
	
	&:hover,
	&.active {
		background #3f9ae3
		color white 
		stroke white
	}
	
	&:focus {
		outline none
	}
	
	&-Pr {
		fill none
		// stroke currentColor
		stroke-linejoin round
		stroke-linecap round
		width 35px 
		height 48px		
		
		@media (min-width $breakpoint-xl) {
			width 60px 
			height 90px
		}
	}
	
	&-Title {
		font-size 12px
		font-weight 600  
		text-align left
		text-transform uppercase
		
		@media (min-width $breakpoint-xl) {
			font-size 14px 
		}
	}
}