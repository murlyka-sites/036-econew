}

.Topic {
	padding-top 20px 
	
	&-Title {
		color #333333
	}
}

.AlertNumber {
	position relative
	border: 1px solid #dddddd;
	padding 15px 20px 
	
	&::before {
		content: ""
		display block
		width 31px
		height 31px
		position absolute
		left 50%
		top 0
		transform translate(-50%, -50%)
		background white url(../images/quote.png) center no-repeat
		
		@media (min-width $sml) {
			left 220px
		}
	}
	
	@media (min-width $sml) {
		display flex
		align-items center
	}
	
	&-Col {
		padding 25px 10px
		
		@media (min-width $sml) {
			padding 0 30px 
		}
		
		@media (min-width $md) {
			padding 0 40px 
		}
		
		&:first-child {
			border-bottom: 1px solid #dddddd;
			
			@media (min-width $sml) {
				border-bottom none
				border-right 1px solid #dddddd
				width 160px 
				flex none
			}
			
			@media (min-width $md) {
				width 200px
			}
		}
	}
	
	&-Val {
		text-align center
		color: #3f9ae3;
		font-size 60px 
		font-weight 600 
		
		@media (min-width: $sm) {
			font-size 80px 
		}
		
		@media (min-width: $sml) {
			font-size 90px 
		}
		
		@media (min-width $mdl) {
			font-size 100px 
		}
	}
	
	&-Units {
		font-weight 400 
		font-size 20px 
	}
	
	&-Desc {
		font-size 14px 
		font-style italic 
		
		@media (min-width: $sm) {
			font-size 16px 
		}
		
		@media (min-width: $mdl) {
			font-size 18px 
		}
	}
}

.Table {
	width 100%
	min-width: 530px
	border: 2px solid #3f9ae3;
	border-spacing 0
	font-size 14px
	
	&-Wrap {
		width: 100%
		overflow-x: auto
	}
	
	&-Hint {
		display flex
		align-items center
		padding 15px 25px
	}
	
	&-Icon {
		margin-right 20px 
	}
	
	&-Label {
		font-size 12px 
	}
	
	&-Row {		
		--separator-color #dddddd
		--separator-h-position center bottom
		--separator-v-position right center
		--separator-h-size calc(100% - 20px) 1px
		--separator-v-size 1px 100%
		
		&:first-child {
			--separator-v-size 1px calc(100% - 10px)
			--separator-v-position right bottom
		}

		&:last-child {
			--separator-h-size 0 0
			--separator-v-size 1px calc(100% - 10px)
			--separator-v-position right top
		}
		
		&:only-child {
			--separator-v-size 1px calc(100% - 20px)
			--separator-v-position right center
		}
	}
	
	&-Head,
	&-Cell {
		
		background-image linear-gradient(to bottom, var(--separator-color), var(--separator-color)), linear-gradient(to right, var(--separator-color), var(--separator-color))
		background-position var(--separator-h-position), var(--separator-v-position)
		background-size var(--separator-h-size), var(--separator-v-size)
		background-repeat no-repeat
		
		&:last-child {
			--separator-v-size 0 0
		}
	}
	
	&-Head {
		--separator-color #1c75bc
	}
	
	// &-Row:first-child {
		
	// 	--separator-v-size 1px calc(100% - 10px)
	// 	--separator-v-position right bottom
	// }
	/*
	&-Row:first-child &-Head,
	&-Row:first-child &-Cell{
		
		&:last-child {
			--separator-v-size 0 0
		}
	}
	
	&-Row:last-child &-Head,
	&-Row:last-child &-Cell{
		--separator-h-size 0 0
		--separator-v-size 1px calc(100% - 10px)
		--separator-v-position right top
		
		&:last-child {
			--separator-v-size 0 0
		}
	}
	
	// &-Row:only-child &-He
	

	
	&-Head {
		--separator-color #1c75bc
	}
	*/
	&-Head {
		text-align left
		color white 
		padding 25px 20px
		font-size 13px 
		font-weight 600 
		line-height 1.4
		background-color #3f9ae3
	}
	
	&-Body {
		margin 10px 
	}
	
	&-Cell {
		padding 20px 
	}
	
	

	
	// &-CellInner  {
	// 	padding 10px
	// 	border-bottom 1px solid #dddddd
	// 	height 100%
	// }
	
